<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="member.model.MemberVO" table="member" >
		
		<id name="mem_no" column="PK_MEMBER">
			<generator class="native">
			</generator>
		</id>
		
		<property name="mem_name" column="mem_name"/>
		<property name="mem_password" column="mem_password"/>
		<property name="mem_email" column="mem_email"/>
		<property name="mem_photo" column="mem_photo"/>
		
	</class>
</hibernate-mapping>